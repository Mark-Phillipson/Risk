{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/api/*",
      "/_framework/*",
      "/_content/*",
      "/lib/*",
      "/js/*",
      "/css/*",
      "/images/*",
      "/favicon*",
      "/*.map",
      "/*.css",
      "/*.js",
      "/*.wasm",
      "/*.json",
      "/*.png",
      "/*.jpg",
      "/*.jpeg",
      "/*.svg",
      "/*.ico"
    ]
  },
  "mimeTypes": {
    ".wasm": "application/wasm"
  }
}
